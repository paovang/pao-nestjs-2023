  "scripts": {
    "cache:clear": "ts-node -r tsconfig-paths/register ./src/infrastructure/adapters/cache-manager/command/clear-cache.ts",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "command-nest": "node dist/main-cli",
    "migration:generate": "typeorm-ts-node-esm migration:generate ./src/infrastructure/adapters/repositories/typeorm/migrations/new-migration -d ./src/infrastructure/adapters/repositories/typeorm/data-source.ts",
    "migration:run": "typeorm-ts-node-esm migration:run -d src/infrastructure/adapters/repositories/typeorm/data-source.ts",
    "migration:revert": "typeorm-ts-node-esm migration:revert -d ./src/infrastructure/adapters/repositories/typeorm/data-source.ts",
    "db:seed": "ts-node src/infrastructure/adapters/repositories/typeorm/seeding/data-source.ts"
  },
